{
  "import as default": {
    "prefix": "imd",
    "body": "import $2 from '$1';",
    "description": "import as default"
  },
  "import as module": {
    "prefix": "imm",
    "body": "import { $2 } from '$1';",
    "description": "import as module"
  },
  "Create useState hook": {
    "prefix": "ush",
    "body": "const [${3}, set${3/(.*)/${3:/upcase}/}] = useState<${1:type}>(${2:defaultValue});$0",
    "description": "Creates React useState hook"
  },
  "destruct assignment value": {
    "prefix": "dv",
    "body": "const { $2 } = $1;",
    "description": "destruct assignment value"
  }
}
